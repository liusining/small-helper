{
  "manifest_version": 2,
  "name": "Small helper",
  "version": "0.2",
  "minimum_chrome_version": "29",
  "browser_action": {
    "default_popup": "popup/popup.html"
  },
  "content_scripts": [{
    "matches": ["https://big.one/accounts"],
    "js": ["content.js"]
  }],
  "permissions": ["tabs"]
}